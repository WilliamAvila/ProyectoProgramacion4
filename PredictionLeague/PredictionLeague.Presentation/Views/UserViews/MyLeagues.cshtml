<h1>My Leagues</h1>
<table class="table table-hover" ng-init="getSuscribedLeagues()">
    <tr class="row lead">
        <th>Id</th>
        <th>Name</th>
        <th>Points</th>

        <th>Subscribed</th>
    </tr>
    <tr class="row" ng-repeat="subscribedLeague in subscribedLeagues">

        <td>  {{subscribedLeague.Id}}  </td>
        <td><a class=" btn-link" href="/league/{{subscribedLeague.Id}}"> {{subscribedLeague.Name}} </a></td>
        <td> 0</td>
        <td>
            <button ng-click="SubscribeLeague(league.name)" class=" {{subscribedLeague.subscribed}}">Unsubscribe </button>
        </td>
</table>
